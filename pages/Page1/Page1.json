              "name": "Validate.get_validation",
              "collectionId": "Page1_Validate",
              "confirmBeforeExecute": false,
              "pluginType": "JS",
              "jsonPathKeys": [
                "submitted_item => {\n  try {\n    let submitted_obj = JSON.parse(submitted_item);\n    let is_validated = true;\n    if (Object.keys(submitted_obj || ({})).length === 0) {\n      return false;\n    }\n    const re = /^\\d{4}[kKdD]$/;\n    Object.entries(submitted_obj).forEach(([k, v]) => {\n      if (!(re.test(k.trim()) && typeof v == 'number' && !isNaN(v))) {\n        is_validated = false;\n      }\n    });\n    return is_validated;\n  } catch (error) {\n    return false;\n  }\n}"
              ],
              "timeoutInMillisecond": 10000
            },
            {
              "id": "Page1_getTransactionSum.get_transaction_sum",
              "name": "getTransactionSum.get_transaction_sum",
              "collectionId": "Page1_getTransactionSum",
              "clientSideExecution": true,
              "confirmBeforeExecute": false,
              "pluginType": "JS",
              "jsonPathKeys": [
                "submitted_item => {\n  try {\n    let sum_credit = 0;\n    let sum_debit = 0;\n    let submitted_obj = JSON.parse(submitted_item);\n    Object.entries(submitted_obj || ({})).forEach(([k, v]) => {\n      if (k.trim().slice(-1) == 'K') sum_credit += v; else sum_debit += v;\n    });\n    return (sum_debit - sum_credit).toFixed(2);\n  } catch (error) {\n    console.log(\"Error in get_transaction_sum\", error);\n    return false;\n  }\n}"
              ],
              "timeoutInMillisecond": 10000
